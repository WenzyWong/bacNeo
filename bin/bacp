#!/bin/bash

print_logo() {
  echo -e "\e[36m"
  echo '
               _                 _   _             ___   ___  
              | |               | \ | |           |__ \ / _ \ 
              | |__   __ _  ___ |  \| | ___  ___    ) || | | |
              | `_ \ / _` |/ __|| . ` |/ _ \/ _ \  / / | | | |
              | |_) | (_| | (__ | |\  |  __/ (_) |/ /__| |_| |
              |____/ \__,_|\___||_| \_|\___|\___/|____(_)___/ 
  '
echo '
               _                    
              | |__   __ _  ___ _ __  
              | `_ \ / _` |/ __| `_ \ 
              | |_) | (_| | (__| |_) |
              |_.__/ \__,_|\___| .__/ 
                               |_|    
'
  echo -e "\e[0m"
}
